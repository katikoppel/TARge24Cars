@model Cars.Models.Cars.CarsDetailsViewModel

<h1>Details</h1>

<div>
	<h4>Cars</h4>
	<hr />
	<dl class="row">
		<div class="form-group row">
			<label asp-for="Id" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.Id)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="Brand" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.Brand)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="Model" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.Model)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="ReleaseYear" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.ReleaseYear)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="Price" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.Price)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.CreatedAt)
		</div>
	</dl>

	<dl class="row">
		<div class="form-group row">
			<label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label>
			@Html.DisplayFor(model => model.ModifiedAt)
		</div>
	</dl>

	<a asp-action="Index" asp-controller="cars" class="btn btn-danger">Back</a>
</div>